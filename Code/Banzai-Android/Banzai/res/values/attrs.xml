<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- REQUIRED FOR HOLOGRAPH -->
    <declare-styleable name="PieGraph">
        <attr name="pieInnerCircleRatio" format="integer" />
        <attr name="pieSlicePadding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="BarGraph">
        <attr name="barAxisColor" format="color" />
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="barShowAxis" format="boolean" />
        <attr name="barShowAxisLabel" format="boolean" />
        <attr name="barShowText" format="boolean" />
        <attr name="barShowPopup" format="boolean" />
    </declare-styleable>
    <!-- custom -->
    <declare-styleable name="ThresholdView">
        <attr name="descText" format="string" />
        <attr name="descTextSize" format="float" />
    </declare-styleable>

</resources>